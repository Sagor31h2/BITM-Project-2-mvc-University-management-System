@using UniversityCourseManagementSystem.Models


@{
    ViewBag.Title = "title";
    Student aStudent = ViewBag.Student;
    Department aDepartment = ViewBag.Department;
}
@section styles{
}
<div class="all-content-wrapper">
<div class="card">
    <u>
        <h2>Student Registration Information</h2><br/><br/></u>
    <form>
        <div class="form-group row">
            <label for="Name" class="col-sm-2 col-form-label">Name</label>
            <div class="col-sm-10">
                <input type="text" readonly class="form-control-plaintext" id="Name" value="@aStudent.Name">
            </div>
        </div>
        <div class="form-group row">
            <label for="Email" class="col-sm-2 col-form-label col-form-label-sm">Email</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aStudent.Email</label>
            </div>
        </div>
        <div class="form-group row">
            <label for="ContactNo" class="col-sm-2 col-form-label col-form-label-sm">Contact No</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aStudent.ContactNo</label>
            </div>
        </div>
        <div class="form-group row">
            <label for="Date" class="col-sm-2 col-form-label col-form-label-sm">Date</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aStudent.Date</label>
            </div>
        </div>
        <div class="form-group row">
            <label for="Address" class="col-sm-2 col-form-label col-form-label-sm">Address</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aStudent.Address</label>
            </div>
        </div>
        <div class="form-group row">
            <label for="Department" class="col-sm-2 col-form-label col-form-label-sm">Department</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aDepartment.DeptName</label>
            </div>
        </div>
        <div class="form-group row">
            <label for="RegistrationNo" class="col-sm-2 col-form-label col-form-label-sm">Registration Number</label>
            <div class="col-sm-10">
                <label class="form-control form-control-sm">@aStudent.RegistrationNo</label>
            </div>
        </div>
        <div class="form-group">
            <b><i><label for="Massage">@ViewBag.Message</label></i></b>
        </div>
        @Html.ActionLink("Back", "Student", "Student")
    </form>
    <table class="table">
        <tbody>
        <tr>
            <td><label>Name</label></td>
            <td><label>@aStudent.Name</label></td>
        </tr>
        </tbody>
    </table>
</div>
</div>
@section scripts
{
    
}